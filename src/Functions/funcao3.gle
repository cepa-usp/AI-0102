size 14 10
set hei 1

begin graph
   math
   
   ! Eixo x
   xaxis min -3 max 3 dticks 3 dsubticks 1
   xaxis lwidth 0.05
   xticks lwidth 0.05
   
   ! Eixo y
   yaxis min -2 max 2
   yaxis lwidth 0.05
   yticks off
   
   ylabels off
   
   ! Função
   !let d1 = 5*x^5+4*x^4+3*x^3+2*x^2+x
   let d1 = x^5+x^4+x^3+x^2+x
   d1 lstyle 1 lwidth 0.1 color blue
   d1 line
   
end graph