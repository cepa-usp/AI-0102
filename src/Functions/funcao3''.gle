size 14 10
set hei 1

begin graph
   math
   
   ! Eixo x
   xaxis min -3 max 3 dticks 3 dsubticks 1
   xaxis lwidth 0.05
   xticks lwidth 0.05
   
   ! Eixo y
   yaxis min -5 max 20
   yaxis lwidth 0.05
   yticks off
   
   ylabels off
   
   ! Função
   let d1 = 20*x^3+12*x^2+6*x+2
   d1 lstyle 1 lwidth 0.1 color blue
   d1 line
   
end graph